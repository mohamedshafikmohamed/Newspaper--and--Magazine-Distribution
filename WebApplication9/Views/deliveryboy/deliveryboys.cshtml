@model IEnumerable<WebApplication9.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<h2>Deliveryboys</h2>

<table class="table">
    <tr>
        <th>
           Username
        </th>
        <th>
           Line
        </th>
       
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
          
            <td>
                @Html.DisplayFor(modelItem => item.line)
            </td>
          
            <td>
              
               
                @Html.ActionLink("Delete "+item.UserName, "delete_deliveryboy", "deliveryboy", new { id = item.Id }, new { @class = "btn btn-danger" })
               
            </td>
            <td>

                @ViewBag.d
            </td>
        </tr>
    }

</table>
